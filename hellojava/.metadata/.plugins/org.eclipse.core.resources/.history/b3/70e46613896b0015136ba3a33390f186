package Skeletons;

class Ghoul extends Monster{
	public Ghoul() {
		System.out.println("3 spooky 5 you");
		noise = "swoosh"; 
	}
	
	public static void mash(Monster m){
		System.out.println("boogity boo");
		m.mash(m);
//		spook(); can't reference a non-static method from static context; 
	}
	
	public void spook(){
		System.out.println("I'm so ghoul: " + noise);
	}
	
	public void haunt() {
		System.out.println("eEeeEerrrRrRrrrr");
		mash(this);
	}
}